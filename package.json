  },
  "homepage": "https://github.com/JesterXL/hm-doc#readme",
  "dependencies": {
    "babylon": "^6.18.0",
    "commander": "^2.18.0",
    "debug": "^4.0.1",
    "glob": "^7.1.3",
    "handlebars": "^4.0.12",
    "hm-parser": "^0.1.5",
    "lodash": "^4.17.11",
    "supports-color": "^5.5.0"
  },
  "devDependencies": {
    "jest": "^23.6.0"
